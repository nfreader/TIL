# Pick from 'local', 'dev', 'test', or 'prod'.
# Defaults to 'prod', disabling error output and template debugging
APP_ENV=local

# Database connection settings. Used to generate a DSN environment variable
DB_METHOD='mysql'
DB_HOST='localhost'
DB_PORT=3306
DB_NAME='database'
DB_USER='user'
DB_PASS='password'

# The name of your application
APP_NAME="WAS Sample Application"

# (Generally) Uses Semver, see https://semver.org/ for guidance.
VERSION_MAJOR=0
VERSION_MINOR=1
VERSION_PATCH=0
VERSION_LABEL='-dev'

# DO NOT EDIT ANYTHING BELOW THIS LINE #
DB_DSN="${DB_METHOD}:host=${DB_HOST};port=${DB_PORT};dbname=${DB_NAME}"
VERSION="${VERSION_MAJOR}.${VERSION_MINOR}.${VERSION_PATCH}${VERSION_LABEL}"